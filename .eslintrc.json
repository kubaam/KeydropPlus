{
  "env": {
    "browser": true,
    "es2021": true
  },
  "globals": {
    "$": "readonly",
    "jQuery": "readonly"
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "warn",
    "semi": "off",
    "no-empty": "off",
    "no-extra-semi": "off",
    "no-unsafe-optional-chaining": "off",
    "no-redeclare": "off",
    "no-constant-condition": "off"
  }
}
